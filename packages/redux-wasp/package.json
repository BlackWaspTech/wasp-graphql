{
  "name": "redux-wasp",
  "version": "0.3.1",
  "description": "Utilize cutting-edge GraphQL APIs without rebuilding your front-end",
  "main": "dist/bundle.js",
  "author": "BlackWaspTech (https://github.com/BlackWaspTech)",
  "license": "MIT",
  "homepage": "https://github.com/BlackWaspTech/redux-wasp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlackWaspTech/redux-wasp.git"
  },
  "bugs": {
    "url": "https://github.com/BlackWaspTech/redux-wasp/issues"
  },
  "keywords": [
    "redux",
    "graphql"
  ],
  "scripts": {
    "test": "jest -i",
    "test:watch": "jest -i --watch",
    "test:coverage": "jest --coverage",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "build": "rollup --c rollup.config.js",
    "prepublish": "npm run build"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupTests.js",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "wasp-graphql": "^0.3.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.1",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.3",
    "husky": "^1.1.3",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^1.6.6",
    "lint-staged": "^8.0.4",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "prettier": "^1.15.2"
  }
}
